<html>
  <head>
    99Cats
  </head>

  
  <body>
    <h1>All Cats</h1>
    <ul>
      <% @cats.each do |cat| %>
        <li>
          <%= link_to "#{cat.id}: #{cat.name}, Age: #{cat.age}", cat_url(cat) %>
        </li>
      <% end %>
    </ul>
    </br>
    <%= link_to "Create a new cat", new_cat_url %>
  </body>


</html>